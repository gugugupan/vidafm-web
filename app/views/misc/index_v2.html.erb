<script type="text/javascript">
(function() {
  $(function() {
    var setBackgroundImgSize, setWrapperPosition;
    setWrapperPosition = function() {
      var shouldBeMarginTop;
      shouldBeMarginTop = ($(window).height() - 61 - 300) / 2;
      return $("#index-wrapper").css({
        marginTop: shouldBeMarginTop
      });
    };
    setBackgroundImgSize = function() {
      var current_height, current_width, ratio;
      current_width = $(window).width();
      current_height = $(window).height();
      ratio = 1281 / 793.0;
      if (current_width / ratio > current_height) {
        return $("#background img").css({
          width: "100%",
          height: ""
        });
      } else {
        return $("#background img").css({
          witdh: "",
          height: "100%"
        });
      }
    };
    setWrapperPosition();
    setBackgroundImgSize();
    $(window).resize(setBackgroundImgSize);
    return $(".screenshots").cycle({
      prev: '#arrow-left',
      next: '#arrow-right'
    });
  });
}).call(this);

</script>

