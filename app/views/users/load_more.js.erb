$("#loadmore").remove();
$("#photo_list_area").append('<%= j render :partial => "moments/item", :collection => @moments["data"] %>');

layoutSubviews( <%= @moments['current_page'].to_i * 20 %> );

<% if @moments['is_last_page'].to_i == 0 %>
	$("#photo_list_area").append('<%= j render :partial => "widgets/load_more", :locals => {:page => params[:page].to_i + 1, :controller => "users", :id => params[:id]} %>');
<% end %>

map.setMarkersWithMoments( <%= raw @moments['data'].to_json %> );
