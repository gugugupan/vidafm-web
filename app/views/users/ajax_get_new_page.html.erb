<% case params[ :type ] -%>
<% when "following" , "followers" -%>
	<%= render :partial => "users/userfeed" , :collection => @user_list , :locals => { :is_self => true } -%>
<% when "commentlike" , "story" -%>
	<%= render :partial => "widgets/feeds" , :collection => @feeds , :locals => { :boxtype => "datebox" } -%>
	<% if @feeds .length > 0 -%>
		<%= render :partial => "widgets/feedsparam" , :locals => { :qparams => @qparams } -%>
	<% end -%>
<% end -%>
