<div class="thumbnail" style="width:245px;">
	<%= link_to image_tag( pic[ "photo" ] [ "url_m" ] , :class => "show-photo" ) , "/moments/#{ pic[ 'moment_id' ] }#activity#{ pic[ 'id' ] }" , :class => "thumb" %>
<% if first %>
	<h1 class="ribbon red"> <%= date %> </h1>
	<%= link_to like_count , like_link , :class => "thumb_like btn_dark btn_small" %>
<% end %>
</div>

