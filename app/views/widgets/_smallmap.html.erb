<% unless moment[ 'lat' ] .nil? || moment[ 'lng' ] .nil? || ( moment[ 'lat' ] == 0 && moment[ 'lng' ] == 0 ) -%>
<a href="javascript:showMap(<%= moment .to_json %>)">
	<%= image_tag "http://ditu.google.cn/maps/api/staticmap?maptype=roadmap&markers=color%3Ared|size%3Amid|#{ moment[ 'lat' ] }%2C#{ moment[ 'lng' ] }&sensor=false&size=418x320&zoom=15" , :alt => "" , :id => "google_map" %>
</a>
<% end -%>