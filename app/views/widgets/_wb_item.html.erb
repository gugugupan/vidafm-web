<div class="thumbnail-pic">    
    <img src="<%= wb_item[ "items" ] [ 0 ] [ "photo" ] [ "url_l" ] %>" alt="dd">
    <div class="rls_container">
        <div class="rls_container_inner">
            <div class="span6">
                <i class="icon-folder-open"></i> <%= wb_item[ "items_count" ] %>
            </div>
            <div class="span6">
                <i class="icon-share-alt"></i> <%= wb_item[ "likes_count" ] %>
            </div>
        </div>
    </div>
</div>
<div class="story_container">
    <%= avatar_tag_b( wb_item[ "user" ] , { :class => "float_avatar" } ) %>
    <div class="story_body">
        <h4 class="story_heading"><a href="/moments/2111451?md=ms"> #随拍 06月05日 </a></h4>
        <p class="story_username">
            作者
        </p>
    </div>
    <div class="clearfix"></div>
</div>
<div class="tencent-share-container">
    <p>
        <a href="#" class="btn-blue-small"><i class="icon-share-alt"></i> 分享赢Q币</a>
    </p>
</div>
